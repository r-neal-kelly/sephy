{
  "name": "sephy",
  "version": "1.0.0",
  "description": "A picture viewing app named after my dearly departed cat.",
  "main": "./src/scripts/js/main.js",
  "scripts": {
    "build": "tsc -b ./src/scripts",
    "rebuild": "tsc -b -f ./src/scripts",
    "start": "npm run build && electron-forge start",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-neal-kelly/sephy.git"
  },
  "author": "r-neal-kelly",
  "license": "CC-BY-NC-SA-4.0",
  "bugs": {
    "url": "https://github.com/r-neal-kelly/sephy/issues"
  },
  "homepage": "https://github.com/r-neal-kelly/sephy#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.59",
    "@electron-forge/maker-deb": "^6.0.0-beta.59",
    "@electron-forge/maker-rpm": "^6.0.0-beta.59",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.59",
    "@electron-forge/maker-zip": "^6.0.0-beta.59",
    "electron": "^13.1.7",
    "esm": "^3.2.25",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "sephy"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
